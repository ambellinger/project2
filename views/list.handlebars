<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
      <div class="card mt-4">
        <div class="card-body">
          <h5 class="card-title">Saved Names</h5>


          {{#each names}}
          {{#if list}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="name/{{this.id}}">{{this.name}}</a>
            <button class="btn btn-danger float-right delete" id="delete">ｘ</button>
          </li>
          {{/if}}
          {{/each}}

          <a href="/">← Back To Home</a>
        </div>
      </div>
    </div>
  </div>
</div>